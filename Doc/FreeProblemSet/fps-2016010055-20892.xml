		<solution language="C"><![CDATA[#include<stdio.h>
int main()
{
	int n,m,i,j,s=0,k=0,t;
	scanf("%d%d",&n,&m);
	if(n>m)
	{
		t=n;n=m;m=t;
	}
	for(i=0;i<n;i++)
		s+=(n-i)*(m-i);
	for(i=0;i<n;i++)
		for(j=0;j<m;j++)
			if(i==j)
				;
			else
			    k+=(n-i)*(m-j);
		printf("%d %d",s,k);
		return 0;
}]]></solution>
