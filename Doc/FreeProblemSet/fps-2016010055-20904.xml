		<solution language="C"><![CDATA[#include<stdio.h>
int main()
{
	long i=1,j=1,k=2,t;
	scanf("%ld",&t);
	while(--t)
	{
		if(k%2==0)
		{
			if(i==1)
			{
				j++;
				k++;
			}
			else
			{
				i--;
				j++;
			}
		}
		else
		{
			if(j==1)
			{
				i++;
				k++;
			}
			else
			{
				i++;
				j--;
			}
		}
	}
	printf("%ld/%ld\n",i,j);
	return 0;
}]]></solution>
